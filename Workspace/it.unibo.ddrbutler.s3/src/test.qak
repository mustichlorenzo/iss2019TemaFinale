	}
	Transition t0 whenMsg schedulingCompleted and "finito" -> waitCmd
				  whenMsg schedulingCompleted and "!finito" -> execMove		  
	
	State execMove {
		delay 1000
		
		forward movementhandler -m movementCmd : movementCmd($NextMove)
